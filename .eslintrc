{
  "extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended", "prettier", "prettier/react"],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "react/prop-types": 0,
    "import/prefer-default-export": 0,
    "no-unused-vars": 0,
    "import/named": 0,
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
  }
}
